# {{ ansible_managed }}

[Unit]
Description="keystone-service-users"
Requires=network-online.target

After=network-online.target

ConditionFileNotEmpty=/etc/keystone/keystone.conf

[Service]
Type=oneshot
RemainAfterExit=no

User=keystone
Group=keystone

ProtectSystem=full
ProtectHome=read-only
PrivateTmp=yes

ExecStart=/usr/local/bin/keystone-service-users

[Install]
WantedBy=multi-user.target
